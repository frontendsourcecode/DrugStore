function updateCart(response) {

        if (response.level === "success" || response.level === "warning") {

            if (response.itemhash && response.quantity) {
              $('.item-' + response.itemhash + ' .cart-quantity').val(response.quantity);
            }

            if ((response.action == "increaseQuantity") || (response.action == "decreaseQuantity")) {
                $('.item-total-' + response.itemhash).html(response.item_total);
            } else if (response.action == "removeItem") {
               var cartcnt = $('#cart_count').html();
               if(cartcnt>0) {
                  cartcnt = cartcnt - 1;
                  $('#cart_count').html(cartcnt);
                }
                $('.item-' + response.itemhash).fadeOut("normal", function () {
                    $(this).remove();
                });
            }
            if (response.empty == true) {
                location.reload();
            } else {
                $('#total').html(response.total);
                $('#sub-total').html(response.sub_total);
                $('#tax').html(response.tax_total);
                $('#cart-header-total').html(response.total);
                $('#header .cart .badge').html(response.cart_count);
            }
        }

    }
