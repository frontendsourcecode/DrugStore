    function zoomtouch(e){
		var x, y;
		// var zoomer = e.currentTarget;
        offsetX = e.touches[0].pageX;
        offsetY = e.touches[0].pageY;
        x = offsetX/(e.touches[0].target.offsetWidth)*100;
		y = offsetY/(e.touches[0].target.offsetHeight)*100;
        // $(".img-show_images").css("background-position",x+"% "+y+"100%");
        // e.style.backgroundPosition = x+'% '+y+'%';
      
	}

	function zoom(e){
		var x, y;
		var zoomer = e.currentTarget;
		if(e.offsetX) {
			offsetX = e.offsetX;
		} else {
			offsetX = e.offsetX;
            // console.log("x");
		}

		if(e.offsetY) {
			offsetY = e.offsetY;
		} else {
			offsetX = e.offsetY;
            // console.log("y");
		}
		x = offsetX/zoomer.offsetWidth*100;
		y = offsetY/zoomer.offsetHeight*100;
		zoomer.style.backgroundPosition = x+'% '+y+'%';
	}
// });

    </script>
    
